{
  "name": "poker-planning-server",
  "version": "0.0.1",
  "description": "Poker planning",
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "js-production": "browserify client/js/app.js -t babelify --verbose --debug --poll | uglifyjs > public/build.min.js && echo 'built'",
    "js-watch": "browserify client/js/app.js -t babelify -o public/build.min.js",
    "css": "cleancss -O2 -o public/style.min.css client/css/*.css && echo 'styled'",
    "build-production": "npm run js-production && npm run css",
    "build-watch": "npm run js-watch && npm run css",
    "watch": "npm run build-watch && node index.js"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "bufferutil": "^3.0.2",
    "express": "^4.15.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "shortid": "^2.2.8",
    "socket.io": "^2.0.3",
    "utf-8-validate": "^3.0.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "clean-css-cli": "^4.1.6",
    "lodash": "^4.17.4",
    "react-router": "^4.2.0",
    "react-router-redux": "^4.0.8",
    "socket.io-client": "^2.0.3",
    "superagent": "^3.6.0",
    "superagent-defaults": "^0.1.14",
    "uglify": "^0.1.5",
    "watchify": "^3.9.0"
  }
}
